let Entry = https://prelude.dhall-lang.org/Map/Entry

let entry = ../../../Map/entry

let Share = ../../Share
let shareVal = ../../share

let InstrumentName = ../Name
let instrumentName = ../name

let share
      : Text -> Double -> Entry InstrumentName Share
      = \(symbol : Text)
     -> \(value : Double)
     -> entry InstrumentName Share (instrumentName symbol) (shareVal value)

in share
