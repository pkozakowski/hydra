let Map/Type = https://prelude.dhall-lang.org/Map/Type

let Time = ../../../Time

let Share = ../../Share

let InstrumentName = ../Name

let Config
      : Type -> Type
      = \(Instrument : Type)
     -> { configs : Map/Type InstrumentName Instrument
        , target : Map/Type InstrumentName Share
        , tolerance : Double
        , updateEvery : Time
        }

in Config
