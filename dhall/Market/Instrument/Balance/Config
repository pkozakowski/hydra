let Map/Type = https://prelude.dhall-lang.org/Map/Type

let Time = ../../../Time

let Asset = ../../Asset
let Share = ../../Share

let Config
      : Type -> Type
      = \(Instrument : Type)
     -> { configs : Map/Type Asset Instrument
        , target : Map/Type Asset Share
        , tolerance : Double
        , updateEvery : Time
        }

in Config
