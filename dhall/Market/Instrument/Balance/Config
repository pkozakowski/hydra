let Map = https://prelude.dhall-lang.org/Map/Type

let Time = ../../../Time/Type

let Share = ../../Share

let InstrumentName = ../Name

let Config
      : Type -> Type
      = \(Instrument : Type)
     -> { configs : Map InstrumentName Instrument
        , target : Map InstrumentName Share
        , tolerance : Double
        , updateEvery : Time
        }

in Config
